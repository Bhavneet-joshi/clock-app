{
  "expo": {
    "name": "Digital Clock",
    "slug": "digital-clock-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.admiasmo.digitalclock",
      "buildNumber": "1",
      "infoPlist": {
        "UIAppFonts": [
          "assets/fonts/digital7mono.ttf",
          "assets/fonts/vector-icons/Ionicons.ttf",
          "assets/fonts/vector-icons/MaterialIcons.ttf",
          "assets/fonts/vector-icons/FontAwesome.ttf"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.admiasmo.digitalclock",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "fonts": [
        {
          "asset": "./assets/fonts/digital7mono.ttf"
        },
        {
          "asset": "./assets/fonts/vector-icons/Ionicons.ttf"
        },
        {
          "asset": "./assets/fonts/vector-icons/MaterialIcons.ttf"
        },
        {
          "asset": "./assets/fonts/vector-icons/FontAwesome.ttf"
        }
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#a4e4a2"
        }
      ],
      "expo-router",
      "expo-updates"
    ],
    "extra": {
      "eas": {
        "projectId": "faeb7ab7-8c84-49a8-aba5-0f54943c4b30"
      }
    },
    "owner": "admiasmo",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/faeb7ab7-8c84-49a8-aba5-0f54943c4b30"
    }
  }
}
